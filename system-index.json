{
  "project_name": "ros2-robot-stack",
  "environment": {
    "host_os": "Linux (WSL 2 Ubuntu 22.04)",
    "execution_runtime": "WSL 2 / Docker",
    "node_version": "v20",
    "python_version": "3.10",
    "ros_distro": "humble",
    "notes": "Project lives in ~/projects/ros2-robot-stack. Use tsx for backend, Bun for installs."
  },
  "services": {
    "frontend": {
      "path": "apps/frontend",
      "runtime": "Next.js 16 (Turbopack)",
      "port": 3000,
      "framework": "React Three Fiber + Tailwind v4"
    },
    "backend": {
      "path": "apps/backend",
      "runtime": "Node.js (tsx)",
      "port": 4000,
      "framework": "Fastify + Socket.IO + rclnodejs"
    },
    "ml-service": {
      "path": "apps/ml-service",
      "runtime": "Python (UV + FastAPI)",
      "port": 8000,
      "status": "skeleton"
    },
    "ros2_ws": {
      "path": "robotics/ros2_ws",
      "runtime": "ROS 2 Humble",
      "packages": ["simulation_manager"]
    }
  },
  "phases": {
    "current": 1,
    "next": 2,
    "total": 8
  }
}
